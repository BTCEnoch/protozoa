  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/three": "^0.177.0",
    "@types/winston": "^2.4.4",
    "@vitejs/plugin-react": "^4.2.1",
